          "editorMode": "code",
          "format": 0,
          "rawSql": "WITH page_views AS (\n  SELECT\n    (SELECT value.string_value FROM UNNEST(event_params) WHERE key = 'page_location') AS page_path,\n    COUNT(*) AS view_count\n  FROM\n    `${GA_PROJECT_ID}.analytics_${GA_DEV_PROPERTY_ID}.events_*`\n  WHERE\n    event_name = 'page_view'\n    AND _TABLE_SUFFIX BETWEEN\n      FORMAT_DATE(\"%Y%m%d\", DATE_SUB(CURRENT_DATE(), INTERVAL 30 DAY)) AND\n      FORMAT_DATE(\"%Y%m%d\", CURRENT_DATE())\n  GROUP BY\n    page_path\n)\n\nSELECT\n  page_path,\n  view_count\nFROM\n  page_views\nORDER BY\n  view_count DESC\nLIMIT 10",
          "refId": "A"
        }
      ],
