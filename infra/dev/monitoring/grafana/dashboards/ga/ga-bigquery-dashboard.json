          "editorMode": "code",
          "format": 0,
          "rawSql": "WITH api_events AS (\n  SELECT\n    event_date,\n    (SELECT value.string_value FROM UNNEST(event_params) WHERE key = 'api_name') AS api_name,\n    (SELECT value.string_value FROM UNNEST(event_params) WHERE key = 'status') AS status,\n    COUNT(*) AS event_count\n  FROM\n    `${GA_PROJECT_ID}.analytics_${GA_DEV_PROPERTY_ID}.events_*`\n  WHERE\n    event_name = 'api_call'\n    AND _TABLE_SUFFIX BETWEEN\n      FORMAT_DATE(\"%Y%m%d\", DATE_SUB(CURRENT_DATE(), INTERVAL 30 DAY)) AND\n      FORMAT_DATE(\"%Y%m%d\", CURRENT_DATE())\n  GROUP BY\n    event_date, api_name, status\n)\n\nSELECT\n  event_date,\n  api_name,\n  status,\n  event_count\nFROM\n  api_events\nORDER BY\n  event_date ASC",
          "refId": "A"
        }
      ],
